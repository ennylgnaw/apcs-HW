public class Mysterior {

    public void swap(int[] arr, int a, int b) {
	int placeholder = arr[a];
	arr[a] = arr[b];
	arr[b] = placeholder;
    }

    public int mystery(int a, int b, int c, int[] arr) {
	int v = arr[c];
	swap(arr, arr[c], arr[b]);
	int s = a;
	for(int i = a; i < b; i++)
	    if (arr[i] < v) {
		swap(arr, arr[s], arr[i]);
		s += 1;
	    }
	swap(arr, arr[b], arr[s]);
	return s;
    }
    
}
